import std::io


def main () {
    let mut open : [mut bool] = alias [false ; new 100];
    
    for pass in 0 .. 100 {
        let mut door = pass;
        while door < 100 {
            open [door] = !open [door];            
            door += pass + 1;
        }
    }

    for door in 0 .. 100 {
        println ("door #",
                 door + 1,
                 if open [door] " is open." else "is closed."        
            );
    }
}
