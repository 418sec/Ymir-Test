
def test () {
    let a = [i32 ; 10U];
    let b = [[i32 ; 10U] ; 10U];
    for i in 0U .. b.len {
        for j in 0U .. b [i].len {
            b [i][j] = cast!i32 (i * b[i].len + j);
        }
    }
    
    let aux = b [9 .. 10].array ();
    println (aux);
    let c = b[0 .. 1].array () ~ aux;
    println (c);
    c = [];
    println (c);
}

def main () {
    test ();
}

