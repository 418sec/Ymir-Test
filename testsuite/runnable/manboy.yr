import core.io;

def a (k : i32, x1 : dg ()-> i32, x2 : i32, x3 : i32, x4 : i32, x5 : i32) -> i32 {
    let b = (dg ()-> i32)::init;
    b = () {
        k -= 1;
        let x = x1 ();
        return a (k, x1, x, x2, x3, x4); 
    };
    
    if (k <= 0)
        return x4 + x5;
    else
        return b ();
}

def main () {
    let c = 1;
    let func = move () {  c = 1; return 1; };
    
    println (a (10, func, -1, -1, 1, 0) );
}
